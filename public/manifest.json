{
  "manifest_version": 3,
  "name": "SMK automator",
  "version": "0.1.0.3",
  "description": "Narzędzie do szybszego uzupełniania danych w SMK. \n Pozwala na wprowadzanie dyżurów i procedur przez import z pliku csv.",
  "icons": {
    "16": "icons/SMK_thumbnail_16.png",
    "32": "icons/SMK_thumbnail_32.png",
    "48": "icons/SMK_thumbnail_48.png",
    "128": "icons/SMK_thumbnail_128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "SMK automator",
    "default_popup": "popup.html"
  },
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_idle",
      "js": [
        "contentScript.js"
      ]
    }
  ]
}